{
    "$schema": "https://json.schemastore.org/tsconfig",
  
    "_version": "18.2.0",
    "extends": "@tsconfig/node18/tsconfig.json",
  
    "compilerOptions": {
      "lib": ["es2023"],
      "module": "node16",
